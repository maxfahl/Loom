{
  "name": "@loom/aml",
  "version": "1.0.0",
  "description": "Agent Memory & Learning System for Loom Framework",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest aml/__tests__ --passWithNoTests",
    "test:acp": "jest tests/acp",
    "test:integration": "jest tests/integration",
    "test:e2e": "jest tests/e2e",
    "test:all": "jest --coverage",
    "orchestrate": "npx ts-node tests/orchestrator/full-orchestration.ts",
    "lint": "eslint aml/**/*.ts tests/**/*.ts",
    "format": "prettier --write \"aml/**/*.ts\" \"tests/**/*.ts\"",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "loom",
    "ai",
    "agent",
    "memory",
    "learning",
    "ml"
  ],
  "author": "Loom Framework",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/pako": "^2.0.4",
    "@types/proper-lockfile": "^4.1.4",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@types/tar": "^6.1.13",
    "acp-sdk": "^1.0.3",
    "axios": "^1.6.5",
    "pako": "^2.1.0",
    "proper-lockfile": "^4.1.2",
    "tar": "^7.5.1",
    "xml2js": "^0.6.2",
    "zod": "^3.22.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
